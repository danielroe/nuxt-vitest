{
  "name": "vitest-environment-nuxt",
  "version": "0.6.6",
  "description": "An vitest environment with support for testing code that needs a Nuxt runtime environment.",
  "repository": "danielroe/vitest-environment-nuxt",
  "keywords": [
    "nuxt",
    "vitest",
    "vitest-environment"
  ],
  "author": {
    "name": "Daniel Roe <daniel@roe.dev>",
    "url": "https://github.com/danielroe"
  },
  "license": "MIT",
  "scripts": {
    "build": "unbuild",
    "stub": "unbuild --stub",
    "prepublishOnly": "pnpm build"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "exports": {
    "./utils": {
      "types": "./dist/utils.d.ts",
      "require": "./dist/utils.cjs",
      "import": "./dist/utils.mjs"
    },
    "./module": {
      "types": "./dist/module.d.ts",
      "require": "./dist/module.cjs",
      "import": "./dist/module.mjs"
    },
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "files": [
    "dist",
    "*.d.ts"
  ],
  "dependencies": {
    "@nuxt/kit": "^3.2.0",
    "@vue/test-utils": "^2.2.8",
    "estree-walker": "^3.0.3",
    "h3": "^1.1.0",
    "happy-dom": "^8.4.4",
    "magic-string": "^0.29.0",
    "ofetch": "^1.0.0",
    "unenv": "^1.0.2"
  },
  "peerDependencies": {
    "vitest": "^0.24.5 || ^0.26.0 || ^0.27.0 || ^0.28.0",
    "vue": "^3.2.45"
  }
}
